{
  "name": "simple-react-app",
  "version": "1.0.0",
  "description": "一个简单的React TypeScript前端应用",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --mode development",
    "start:dev": "node start-dev.js",
    "start:mock": "set REACT_APP_USE_MOCK=true && npm start",
    "start:no-mock": "set REACT_APP_USE_MOCK=false && npm start",
    "build": "webpack --mode production"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-loader": "^9.1.0",
    "body-parser": "^2.2.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.0",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "keywords": [
    "react",
    "typescript",
    "frontend",
    "webpack"
  ],
  "author": "",
  "license": "MIT"
}
